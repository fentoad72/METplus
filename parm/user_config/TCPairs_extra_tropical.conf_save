#TC_PAIRS_BDECK_TEMPLATE = {date?fmt=%Y%m}/b{basin?fmt=%s}q{date?fmt=%Y%m}*.gfso.{cyclone?fmt=%s}
#TC_PAIRS_ADECK_TEMPLATE = {date?fmt=%Y%m}/a{basin?fmt=%s}q{date?fmt=%Y%m}*.gfso.{cyclone?fmt=%s}
#TC_PAIRS_BDECK_TEMPLATE = {date?fmt=%Y%m}/b{basin?fmt=%s}q{date?fmt=%Y%m}*.gfso.{cyclone?fmt=%s}
### sort cyclones by storm
#TC_PAIRS_ADECK_TEMPLATE = adeck.gfso.atcf_gen.glbl.{cyclone?fmt=%s}.dat
#TC_PAIRS_BDECK_TEMPLATE = bdeck.gfso.atcf_gen.glbl.{cyclone?fmt=%s}.dat
#TC_PAIRS_OUTPUT_TEMPLATE = {date?fmt=%Y%m}/mlq{cyclone?fmt=%s}.gfso
